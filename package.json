{
    "name": "applet-taro-cli",
    "version": "1.0.0",
    "private": true,
    "description": "小程序的基础脚手架",
    "templateInfo": {
        "name": "vue3-NutUI",
        "typescript": true,
        "css": "sass"
    },
    "scripts": {
        "build:weapp": "cross-env NODE_ENV=development node ./config/init.js && cross-env NODE_ENV=development taro build --type weapp",
        "build:test": "cross-env NODE_ENV=testing node ./config/init.js && cross-env NODE_ENV=testing taro build --type weapp",
        "build:pro": "cross-env NODE_ENV=production node ./config/init.js && cross-env NODE_ENV=production taro build --type weapp",
        "build:swan": "taro build --type swan",
        "build:alipay": "taro build --type alipay",
        "build:tt": "taro build --type tt",
        "build:h5": "taro build --type h5",
        "build:rn": "taro build --type rn",
        "build:qq": "taro build --type qq",
        "build:quickapp": "taro build --type quickapp",
        "dev:weapp": "npm run build:weapp -- --watch",
        "dev:swan": "npm run build:swan -- --watch",
        "dev:alipay": "npm run build:alipay -- --watch",
        "dev:tt": "npm run build:tt -- --watch",
        "dev:h5": "npm run build:h5 -- --watch",
        "dev:rn": "npm run build:rn -- --watch",
        "dev:qq": "npm run build:qq -- --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch",
        "prepare": "husky install"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.7.7",
        "@nutui/nutui-taro": "^3.1.8",
        "@tarojs/components": "3.4.0-beta.3",
        "@tarojs/plugin-framework-vue3": "^3.4.3",
        "@tarojs/plugin-html": "3.4.0-beta.3",
        "@tarojs/runtime": "3.4.0-beta.3",
        "@tarojs/taro": "3.4.0-beta.3",
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@tarojs/cli": "3.4.0-beta.3",
        "@tarojs/mini-runner": "3.4.0-beta.3",
        "@tarojs/webpack-runner": "3.4.0-beta.3",
        "@types/webpack-env": "^1.13.6",
        "@typescript-eslint/eslint-plugin": "^2.x",
        "@typescript-eslint/parser": "^2.x",
        "@vue/compiler-sfc": "^3.0.0",
        "babel-plugin-import": "^1.13.3",
        "babel-preset-taro": "3.4.0-beta.3",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^6.8.0",
        "eslint-config-taro": "3.4.0-beta.3",
        "eslint-plugin-vue": "^7.0.0",
        "husky": "^7.0.0",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.0",
        "stylelint": "9.3.0",
        "typescript": "^3.7.0",
        "vue-loader": "^16.0.0-beta.8"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    },
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    }
}
